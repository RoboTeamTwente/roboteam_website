<div class="container">
  <div class="col-12 mt-5">
    <h1> RoboTeam Twente 2018/2019 </h1>

    <div class="row">
    {{#each model as |member|}}

    <div class="col-6 col-sm-4 col-md-3 col-xl-2">
      <img class="mt-3" src={{member.imageSrc}} style="width: 100%"/>
      <i> {{member.name}} </i> <br>
      {{member.role}}
    </div>
      {{/each}}
    </div>
  </div>
</div>
